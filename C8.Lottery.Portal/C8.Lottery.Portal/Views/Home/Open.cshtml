@using C8.Lottery.Public
@using C8.Lottery.Model
@{
    Layout = null;
    UserInfo user = ViewBag.UserInfo;
    SiteSetting site = ViewBag.SiteSetting;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=yes">
    <title>@site.Name</title>
    <meta name="keywords" content="@site.KeyWord">
    <meta name="description" content="@site.Description">
    <link rel="stylesheet" type="text/css" href="@ViewBag.osshost/css/style.css" />
    <script type="text/javascript" src="@ViewBag.osshost/js/jquery.js"></script>
    <script type="text/javascript" src="@ViewBag.osshost/js/yjp.js"></script>
    <script type="text/javascript" src="@ViewBag.osshost/js/runTime3.js"></script>
    <style>
        .ind_mainUl2 {
            display: none;
        }
    </style>
    <script type="text/javascript" src="/js/baidu-statistics.js"></script>
</head>
<body style="background:#f3f4f4;">
    <header class="hjc_header">
        <a href="/Home/Index" class="hjc_back"></a>
        <a href="javascript:;" class="hjc_logo">彩票开奖查询</a>
        <a href="javascript:;" class="ZX_Sidebar"></a>
    </header>
    <div class="hjc_hdNav">

        <!-- 新增类型选择 By LHS 2018-3-2 -->
        <div class="hdNav_cai_collect">
            <div class="hdNav_cai_lhs_expand"><img src="/images/icon-expand.png"></div>
            <ul class="hdNav_cai_lhs">
                <div style="clear:both;"></div>
            </ul>
        </div>
        <div class="hdNav_cai_expand" style="display:none">
            <ul class="hdNav_cai">

                <div style="clear:both;"></div>
            </ul>
        </div>
    </div>
    <div style="height:500px; display:none" id="kongbaiDiv"></div>
    <div class="hjc_indexmain" style="margin-top:91px;">

    </div>


    <!--侧边栏-->
    @Html.Partial("Sidebar")

    @Html.Partial("Footer")

    <script type="text/javascript" src="@ViewBag.osshost/js/ossjs/HomeIndex.js"></script>

</body>
@{Html.RenderAction("index", "PvUvIp");}
</html>