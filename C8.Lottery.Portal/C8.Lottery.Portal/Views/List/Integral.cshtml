@{
    ViewBag.Title = "Integral";
}

<!doctype html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=yes">
    <title>积分榜</title>

    <link rel="stylesheet" type="text/css" href="~/css/style.css" />
    <link href="~/css/null.css" rel="stylesheet" />
    <link href="~/css/dialog.css" rel="stylesheet" />

    <script src="@ViewBag.osshost/js/jquery-1.8.3.min.js"></script>
    <script src="@ViewBag.osshost/js/dialog.min.js"></script>
    <script type="text/javascript" src="@ViewBag.osshost/js/yjp.js"></script>
    <script type="text/javascript" src="@ViewBag.osshost/js/baidu-statistics.js"></script>
    <script type="text/javascript" src="@ViewBag.osshost/js/ossjs/Integral.js"></script>

</head>

<body style="background:#F4F4F5; overflow: auto;
  -webkit-overflow-scrolling: touch;">

    <header class="hjc_header">
        <a href="/List/Index" class="hjc_back"></a>
        <a href="javascript:;" class="hjc_logo">积分榜</a>
    </header>

    <div class="guessing_header">
        <ul class="C8_nav2 C8_bang">
            <li data-idx="day">
                <a href="javascript:;">
                    <p>日榜</p><span></span>
                </a>
            </li>
            <li data-idx="week">
                <a href="javascript:;">
                    <p>周榜</p><span></span>
                </a>
            </li>
            <li data-idx="month">
                <a href="javascript:;">
                    <p>月榜</p><span></span>
                </a>
            </li>
            <li data-idx="all" class="current">
                <a href="javascript:;">
                    <p>总榜</p><span></span>
                </a>
            </li>
            <div style="clear:both;"></div>
        </ul>
    </div>

    <div class="Ranking_content" style="margin-top: 80px;">
        <div class="Ranking_Cinfo">

        </div>

    </div>

    <div class="Ranking_footer">
        <dl class="Rank_ftDL"></dl>
    </div>


</body>

@{Html.RenderAction("index", "PvUvIp");}
</html>
